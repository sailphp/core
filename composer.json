{
  "name": "sailphp/core",
  "description": "SailFramework",
  "type": "framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Brad Veryard",
      "email": "bradveryard@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1.0",
    "ext-json": "*",
    "illuminate/config": "^9.36.4",
    "illuminate/database": "^9.36.4",
    "illuminate/pagination": "^9.36.4",
    "illuminate/validation": "^9.36.4",
    "illuminate/translation": "^9.36.4",
    "relay/relay": "~2.1.2",
    "respect/validation": "~2.3.7",
    "swiftmailer/swiftmailer": "^6.0",
    "symfony/console": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/filesystem": "^6.4",
    "symfony/finder": "^6.4",
    "symfony/http-foundation": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/psr-http-message-bridge": "^2.1",
    "symfony/routing": "^6.4",
    "symfony/security-csrf": "^5.3",
    "twig/twig": "~3.14.0",
    "vlucas/phpdotenv": "^5.6.1",
    "laminas/laminas-diactoros": "^3.3.1"
  },
  "require-dev": {
    "symfony/var-dumper": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "SailPHP\\": "src/SailPHP/"
    },
    "files": [
      "src/SailPHP/helpers.php"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
