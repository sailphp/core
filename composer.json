{
  "name": "sailphp/core",
  "description": "SailFramework",
  "type": "framework",
  "license": "MIT",
  "authors": [{
    "name": "Brad Veryard",
    "email": "bradveryard@gmail.com"
  }],
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "illuminate/config": "~8.8.0",
    "illuminate/database": "~8.8.0",
    "illuminate/pagination": "~8.8.0",
    "relay/relay": "~2.0",
    "respect/validation": "~1.1",
    "swiftmailer/swiftmailer": "6.0",
    "symfony/console": "^5.2",
    "symfony/dependency-injection": "^5.2",
    "symfony/filesystem": "^5.2",
    "symfony/finder": "^5.2",
    "symfony/http-foundation": "^5.2",
    "symfony/http-kernel": "^5.2.0",
    "symfony/psr-http-message-bridge": "~2.0.2",
    "symfony/routing": "^5.2",
    "twig/twig": "~2.7",
    "vlucas/phpdotenv": "~5.2.0",
    "zendframework/zend-diactoros": "~2.1"
  },
  "require-dev": {
    "symfony/var-dumper": "~4.2"
  },
  "autoload": {
    "psr-4": {
      "SailPHP\\": "src/SailPHP/"
    },
    "files": [
      "src/SailPHP/helpers.php"
    ]
  },
  "config": {
    "sort-packages": true
  }
}
