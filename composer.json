{
  "name": "sailphp/core",
  "description": "SailFramework",
  "type": "framework",
  "license": "MIT",
  "authors": [{
    "name": "Brad Veryard",
    "email": "bradveryard@gmail.com"
  }],
  "require": {
    "php": "^7.1.3",
    "illuminate/config": "~5.8",
    "illuminate/database": "~5.8",
    "illuminate/pagination": "^5.8",
    "relay/relay": "^2.0",
    "respect/validation": "^1.1",
    "swiftmailer/swiftmailer": "6.0",
    "symfony/console": "^4.2",
    "symfony/dependency-injection": "4.1.12",
    "symfony/filesystem": "^3.1",
    "symfony/finder": "^3.1",
    "symfony/http-foundation": "^4.2",
    "symfony/psr-http-message-bridge": "^1.2",
    "symfony/routing": "^4.2",
    "symfony/security": "^4.2",
    "twig/twig": "^2.7",
    "vlucas/phpdotenv": "^3.3",
    "zendframework/zend-diactoros": "^2.1",
    "ext-json": "*"
  },
  "require-dev": {
    "symfony/var-dumper": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "SailPHP\\": "src/SailPHP/"
    },
    "files": [
      "src/SailPHP/helpers.php"
    ]
  },
  "config": {
    "sort-packages": true
